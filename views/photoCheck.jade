include mixins
extends layout
block content 
            include navbar
            h3 Cardholder picture gallery

            form(method='post')    
              h5 Last Name:
                input(type='directory', name='empIDSearch', value='#{imageLast}')
                button.btn.btn-primary(type="submit") Search gallery
                br
                h5(style="color: #CC3300;") #{sess.photoCheckError}
                h5(style="color: #CC3300;") #{sess.photoCheckError1}
                each image in images
                 img(src=image) 
              br
              br
              include footer




      
     
     

      